#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -x "$basedir//" ]; then
  exec "$basedir//" user/bin/env node "$basedir/../@ayekh8/cli-gussesing-game/index.js" "$@"
else 
  exec / user/bin/env node "$basedir/../@ayekh8/cli-gussesing-game/index.js" "$@"
fi
